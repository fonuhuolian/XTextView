<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="FoldTextView">
        <attr name="foldedTextHints" format="string|reference" />
        <attr name="hintWordsColor" format="color|reference" />
        <attr name="hintWordsSize" format="integer" />
        <attr name="textContent" format="string|reference" />
        <attr name="limitRows" format="integer" />
        <attr name="unFoldedTextHints" format="string|reference" />
        <attr name="wordsColor" format="color|reference" />
        <attr name="wordsSize" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="isSupportContentClick" format="boolean" />
    </declare-styleable>

    <declare-styleable name="XPraiseTextView">
        <attr name="x_praise_textColor" format="color" />
        <attr name="x_praise_Icon" format="reference" />
        <attr name="x_praise_textSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="XCommentTextView">
        <attr name="x_comment_name_textColor" format="color" />
        <attr name="x_comment_content_textColor" format="color" />
        <attr name="x_comment_textSize" format="dimension" />
    </declare-styleable>

</resources>